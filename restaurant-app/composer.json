{
    "name": "restaurant-app",
    "description": "A web application for managing a restaurant, including reservations, menu management, and user authentication.",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-mysqli": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}